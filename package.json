{
  "name": "g-mini",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "dev:api": "tsx dev-server.js",
    "dev:full": "concurrently \"pnpm dev\" \"pnpm dev:api\" --names \"vite,api\" --prefix-colors \"cyan,magenta\"",
    "build": "tsc -b && vite build",
    "build:skip-ts": "vite build",
    "lint": "eslint --ext .ts,.tsx --max-warnings=0 .",
    "lint:fix": "eslint --ext .ts,.tsx --fix .",
    "lint:staged": "lint-staged",
    "lint:precision": "eslint --rule \"precision/no-native-arithmetic: error\" \"src/**/*.{ts,tsx}\"",
    "test:precision": "vitest run 'src/__tests__/precision-*.test.ts'",
    "validate:precision": "pnpm run lint:precision && pnpm run test:precision",
    "preview": "vite preview",
    "test": "vitest --exclude='**/performance/**' --exclude='**/stress/**'",
    "test:run": "vitest run --exclude='**/performance/**' --exclude='**/stress/**'",
    "test:with-logs": "vitest run --exclude='**/performance/**' --exclude='**/stress/**' --reporter=verbose",
    "test:debug": "vitest run --exclude='**/performance/**' --exclude='**/stress/**' --reporter=verbose --logHeapUsage",
    "test:logs-env": "cross-env VITEST_LOG_TESTS=true vitest run --exclude='**/performance/**' --exclude='**/stress/**'",
    "test:eventbus:full": "vitest run src/lib/events/__tests__ --reporter=verbose",
    "test:eventbus:log": "pnpm run test:eventbus:full 2>&1 | tee test-results/eventbus-log.txt",
    "test:eventbus:json": "vitest run src/lib/events/__tests__ --reporter=json > test-results/eventbus-json.json 2>&1",
    "test:with-file": "vitest run --reporter=verbose --exclude='**/performance/**' --exclude='**/stress/**'",
    "test:coverage": "vitest run --coverage --exclude='**/performance/**' --exclude='**/stress/**'",
    "test:all": "vitest run",
    "test:fast": "vitest run --exclude='**/performance/**' --exclude='**/stress/**' --exclude='**/integration/**'",
    "test:ci": "vitest run --exclude='**/performance/**' --exclude='**/stress/**'",
    "test:eventbus": "vitest run src/lib/events/__tests__",
    "test:eventbus:unit": "vitest run src/lib/events/__tests__/unit",
    "test:eventbus:integration": "vitest run src/lib/events/__tests__/integration",
    "test:eventbus:performance": "vitest run src/lib/events/__tests__/performance",
    "test:eventbus:stress": "vitest run src/lib/events/__tests__/stress",
    "test:eventbus:business": "vitest run src/lib/events/__tests__/business",
    "test:eventbus:watch": "vitest src/lib/events/__tests__",
    "test:eventbus:coverage": "vitest run --coverage src/lib/events/__tests__",
    "prepare": "husky install",
    "e2e": "playwright test",
    "e2e:ui": "playwright test --ui",
    "e2e:headed": "playwright test --headed",
    "e2e:debug": "playwright test --debug",
    "e2e:report": "playwright show-report",
    "e2e:chrome": "playwright test --project=chrome --headed",
    "e2e:chrome:ui": "playwright test --project=chrome --ui",
    "e2e:auth": "playwright test tests/e2e/with-auth.spec.ts --headed --config=playwright.auth.config.ts",
    "e2e:setup": "playwright test --project=setup --headed",
    "e2e:with-session": "playwright test --project=authenticated --headed",
    "e2e:full": "playwright test --project=setup --project=authenticated --headed",
    "e2e:alerts": "playwright test tests/e2e/alerts-toast-progress.spec.ts --project=authenticated --headed",
    "e2e:alerts:ui": "playwright test tests/e2e/alerts-toast-progress.spec.ts --project=authenticated --ui",
    "e2e:alerts:debug": "playwright test tests/e2e/alerts-toast-progress.spec.ts --project=authenticated --debug",
    "e2e:materials": "playwright test tests/e2e/materials --project=authenticated",
    "e2e:materials:all": "playwright test tests/e2e/materials",
    "e2e:materials:ui": "playwright test tests/e2e/materials --project=authenticated --ui",
    "e2e:materials:debug": "playwright test tests/e2e/materials --project=authenticated --debug",
    "seed:materials": "tsx tests/setup/seed-materials.ts"
  },
  "dependencies": {
    "@chakra-ui/react": "^3.30.0",
    "@dnd-kit/core": "^6.3.1",
    "@dnd-kit/modifiers": "^9.0.0",
    "@dnd-kit/sortable": "^10.0.0",
    "@dnd-kit/utilities": "^3.2.2",
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.0",
    "@heroicons/react": "^2.2.0",
    "@hookform/resolvers": "^5.2.1",
    "@supabase/ssr": "^0.6.1",
    "@supabase/supabase-js": "^2.76.0",
    "@tanstack/react-query": "^5.90.12",
    "@tanstack/react-query-devtools": "^5.91.1",
    "@tanstack/react-virtual": "^3.13.12",
    "@types/decimal.js": "^0.0.32",
    "@types/leaflet-draw": "^1.0.13",
    "chart.js": "^4.5.0",
    "date-fns": "^4.1.0",
    "decimal.js": "^10.6.0",
    "framer-motion": "^12.23.11",
    "immer": "^10.1.1",
    "leaflet": "^1.9.4",
    "leaflet-draw": "^1.0.4",
    "mercadopago": "^2.11.0",
    "qrcode": "^1.5.4",
    "react": "^19.1.0",
    "react-chartjs-2": "^5.3.0",
    "react-dnd": "^16.0.1",
    "react-dnd-html5-backend": "^16.0.1",
    "react-dom": "^19.1.0",
    "react-hook-form": "^7.62.0",
    "react-leaflet": "^5.0.0",
    "react-leaflet-draw": "^0.21.0",
    "react-router-dom": "^7.7.1",
    "recharts": "^3.2.1",
    "typecheck": "^0.1.2",
    "zod": "^4.1.5",
    "zustand": "^5.0.7"
  },
  "devDependencies": {
    "@eslint/js": "^9.30.1",
    "@playwright/test": "^1.56.1",
    "@testing-library/jest-dom": "^6.6.4",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/express": "^5.0.6",
    "@types/leaflet": "^1.9.21",
    "@types/qrcode": "^1.5.6",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "@typescript-eslint/eslint-plugin": "^8.39.1",
    "@typescript-eslint/parser": "^8.39.1",
    "@vercel/node": "^5.5.16",
    "@vitejs/plugin-react-swc": "^3.10.2",
    "@vitest/coverage-v8": "3.2.4",
    "@welldone-software/why-did-you-render": "^10.0.1",
    "concurrently": "^9.2.1",
    "cross-env": "^10.0.0",
    "eslint": "^9.30.1",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "express": "^5.2.1",
    "globals": "^16.3.0",
    "husky": "^8.0.0",
    "jsdom": "^26.1.0",
    "lint-staged": "^16.1.5",
    "react-scan": "^0.4.3",
    "rollup-plugin-visualizer": "^6.0.3",
    "tsx": "^4.21.0",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.35.1",
    "vite": "^7.1.9",
    "vite-bundle-analyzer": "^1.1.0",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^3.2.4"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "eslint --fix --ext .ts,.tsx"
    ]
  }
}
